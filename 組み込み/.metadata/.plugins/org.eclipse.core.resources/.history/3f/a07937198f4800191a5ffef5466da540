void Calibration(){
	while(HAL_GPIO_ReadPin(GPIOC,GPIO_PIN_15)==1){
	motor_move(0,0,0);
	oled_returnhome();
	oled_puts("Calib select    ");
	oled_setcursor(1,0);
	oled_puts("turn on startSW ");
	HAL_Delay(5);
    if(HAL_GPIO_ReadPin(GPIOF,GPIO_PIN_9)==0){	//press RS
    DFPlayer_playmp3(8);
	oled_clear();
	oled_returnhome();
	oled_puts("Angular velocity");
	oled_setcursor(1,0);
	oled_puts("    Calibration ");
	JY901_calibration(2000,1);
	oled_clear();
	oled_returnhome();
	oled_puts("Magnetic");
	oled_setcursor(1,0);
	oled_puts(" Calibration  RS");
	xprintf("houi cab\r\n");
	motor_move(200,0,200);
	JY901_calibration(12000,2);
	motor_move(0,0,0);
	oled_clear();
	oled_returnhome();
	oled_puts("Calibration End ");
	xprintf("cab end\r\n");
	JY901_calibration(100,3);
	xprintf("save\r\n");
	JY901_calibration(100,4);
	DFPlayer_stopmp3();
}
}
}


void HAL_UART_RxCpltCallback(UART_HandleTypeDef *huart){
	if(Rxbuf[0] == 251){
		data[0]=Rxbuf[1];
		data[1]=Rxbuf[2];
	}
	if(Rxbuf[1] == 251){
		data[0]=Rxbuf[2];
		data[1]=Rxbuf[0];
	}
	if(Rxbuf[2] == 251){
		data[0]=Rxbuf[0];
		data[1]=Rxbuf[1];
	}
}


