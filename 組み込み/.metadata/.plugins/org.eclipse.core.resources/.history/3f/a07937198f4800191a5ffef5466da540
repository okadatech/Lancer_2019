/* Private variables ---------------------------------------------------------*/

/* USER CODE BEGIN PV */
/* Private variables ---------------------------------------------------------*/

/* USER CODE END PV */

/* Private function prototypes -----------------------------------------------*/
void SystemClock_Config(void);

/* USER CODE BEGIN PFP */
/* Private function prototypes -----------------------------------------------*/
void HAL_UART_RxCpltCallback(UART_HandleTypeDef *huart);
void Calibration();
void state_print();
void test();
void main_run();
void test2();
/* USER CODE END PFP */

/* USER CODE BEGIN 0 */
uint8_t mode;
uint8_t shyuukai=0;

 /* USER CODE END 0 */

/**
  * @brief  The application entry point.
  *
  * @retval None
  */
int main(void)
{
  /* USER CODE BEGIN 1 */

  /* USER CODE END 1 */

  /* MCU Configuration----------------------------------------------------------*/

  /* Reset of all peripherals, Initializes the Flash interface and the Systick. */
  HAL_Init();

  /* USER CODE BEGIN Init */

  /* USER CODE END Init */

  /* Configure the system clock */
  SystemClock_Config();

  /* USER CODE BEGIN SysInit */

  /* USER CODE END SysInit */

  /* Initialize all configured peripherals */
  MX_GPIO_Init();
  MX_DMA_Init();
  MX_ADC1_Init();
  MX_ADC2_Init();
  MX_ADC3_Init();
  MX_USART1_UART_Init();
  MX_ADC4_Init();
  MX_USART2_UART_Init();
  MX_USART3_UART_Init();
  MX_TIM1_Init();
  MX_TIM2_Init();
  MX_TIM6_Init();
  MX_I2C1_Init();
  MX_UART4_Init();
  MX_TIM8_Init();
  MX_TIM4_Init();
  MX_TIM3_Init();
  MX_TIM17_Init();
  MX_TIM15_Init();
  MX_TIM16_Init();
  /* USER CODE BEGIN 2 */
  xprintf_init(&huart1);
  JY901_init(&hi2c1);
  oled_i2c_to_oled(&hi2c1);
  DFPlayer_init(&huart4);
  oled_returnhome();
  oled_puts("Is The Order a");
  oled_setcursor(1,2);
  oled_puts("Lancer Robot?");
  HAL_Delay(500);
  DFPlayer_setvolume(0x12);
  HAL_Delay(500);
  DFPlayer_playmp3(1);
  HOUI_def=JY901_def_set();

  if(HAL_TIM_Encoder_Init(&htim3, &sConfig) != HAL_OK){Error_Handler();}
  HAL_TIM_Encoder_Start(&htim3, TIM_CHANNEL_ALL);
  TIM3->CNT=30000;


  if(HAL_TIM_Encoder_Init(&htim4, &sConfig) != HAL_OK){Error_Handler();}
  HAL_TIM_Encoder_Start(&htim4, TIM_CHANNEL_ALL);
  TIM4->CNT=30000;

  if(HAL_TIM_Encoder_Init(&htim8, &sConfig) != HAL_OK){Error_Handler();}
  HAL_TIM_Encoder_Start(&htim8, TIM_CHANNEL_ALL);
  TIM8->CNT=30000;


  if (HAL_ADC_Start_DMA(&hadc1, (uint32_t *)ADCdata1,9) != HAL_OK){Error_Handler();}

  if (HAL_ADC_Start_DMA(&hadc2, (uint32_t *)ADCdata2,sizeof(ADCdata2)) != HAL_OK){Error_Handler();}

  if (HAL_ADC_Start_DMA(&hadc3, (uint32_t *)ADCdata3,sizeof(ADCdata3)) != HAL_OK){Error_Handler();}

  if (HAL_ADC_Start_DMA(&hadc4, (uint32_t *)ADCdata4,sizeof(ADCdata4)) != HAL_OK){Error_Handler();}

  if (HAL_UART_Receive_DMA(&huart2, (uint8_t *)Rxbuf,sizeof(Rxbuf)) != HAL_OK){Error_Handler();}

  HOUI_def=JY901_def_set();
  /* USER CODE END 2 */

  /* Infinite loop */
  /* USER CODE BEGIN WHILE */
  while (1)
  {

  /* USER CODE END WHILE */

  /* USER CODE BEGIN 3 */
	  mofu:
	  	  	 mode=//HAL_GPIO_ReadPin(GPIOE,GPIO_PIN_6)*4+
	  	  			 HAL_GPIO_ReadPin(GPIOC,GPIO_PIN_13)*2 +HAL_GPIO_ReadPin(GPIOC,GPIO_PIN_14);
	  	  	  switch(mode){
	  	  	  case 0:
	  	  		  if(HAL_GPIO_ReadPin(GPIOC,GPIO_PIN_15)==1){
	  	  			  HOUI_def=JY901_def_set();
	  	  			  while(HAL_GPIO_ReadPin(GPIOF,GPIO_PIN_9)){
							servo(90,100,90,90);
							ics_set_pos1(0,map_a(90,0,180,4833,10166));
							ics_set_pos1(1,map_a(90,0,180,4833,10166));
							motor_move(0,0,0);
							xprintf("digree=%d\r\n",line_get_data_poorly_made());
							oled_returnhome();
							oled_puts("main_run waiting ");
							oled_setcursor(1,0);
							oled_puts("     for start   ");
							if(HAL_GPIO_ReadPin(GPIOC,GPIO_PIN_15)==0){goto mofu;}
							HAL_GPIO_TogglePin(GPIOD,GPIO_PIN_10);
	  	  			  	}
	  	  			  	  DFPlayer_setvolume(0x1F);
	  	  			  	oled_clear();
	  	  			    oled_returnhome();
	  	  			    oled_puts("main_run select ");
	  	  			    oled_setcursor(1,0);
	  	  			    oled_puts("running");
	  	  			    HOUI_def=JY901_def_set();
					    TIM3->CNT=3000;
					    TIM4->CNT=3000;
					    TIM8->CNT=3000;
					    marker_count_data=0;
					    marker_count_data_=0;
					    while(HAL_GPIO_ReadPin(GPIOF,GPIO_PIN_9)==0){
	  	  			  		main_run();
	  	  			  	}
	  	  				servo(90,100,90,90);
		  	  			ics_set_pos1(0,map_a(90,0,180,4833,10166));
		  	  			ics_set_pos1(1,map_a(90,0,180,4833,10166));
	  	  				motor_move(0,0,0);
	  	  		  }
	  	  		  else{
	  	  			  servo(90,100,90,90);
	  	  			  ics_set_pos1(0,map_a(90,0,180,4833,10166));
	  	  			  ics_set_pos1(1,map_a(90,0,180,4833,10166));
	  	 	  	  	  motor_move(0,0,0);
	  	  			  oled_returnhome();
	  	  			  oled_puts("main_run select");
	  	  			  oled_setcursor(1,0);
	  	  			  oled_puts("OK?     waiting ");
	  	  			  HAL_Delay(5);
	  	  		  HAL_GPIO_TogglePin(GPIOD,GPIO_PIN_10);
	  	  		  }
	  	  	  break;

	  	  	  case 1:
	  	  		  if(HAL_GPIO_ReadPin(GPIOC,GPIO_PIN_15)==1){
  	  			  test();
  	  			  servo(90,100,90,90);
  	  			  //ics_set_pos1(0,map_a(90,0,180,4833,10166));
  	  			  //ics_set_pos1(1,map_a(90,0,180,4833,10166));
  	 	  	  	  motor_move(0,0,0);
  	  			  oled_returnhome();
  	  			  oled_puts("testmode1 select");
  	  			  oled_setcursor(1,0);
  	  			  oled_puts("running         ");
  	  		  }
  	  		  else{
  	  			  servo(90,100,90,90);
  	  			  ics_set_pos1(0,map_a(90,0,180,4833,10166));
  	  			  ics_set_pos1(1,map_a(90,0,180,4833,10166));
  	 	  	  	  motor_move(0,0,0);
  	  			  oled_returnhome();
  	  			  oled_puts("testmode1 select");
  	  			  oled_setcursor(1,0);
  	  			  oled_puts("OK?     waiting ");
  	  			  HAL_Delay(5);
  	  			xprintf(" Rx[0]=%d", Rxbuf[0]);
  	  				  xprintf(" Rx[1]=%d", Rxbuf[1]);
  	  				  xprintf(" Rx[2]=%d", Rxbuf[2]);
  	  				  xprintf(" data[0]=%d", data[0]);
  	  				  xprintf(" data[1]=%d", data[1]);
  	  				  	  xprintf("\r\n");
  	  		  HAL_GPIO_TogglePin(GPIOD,GPIO_PIN_10);
  	  		  }

	  	  	  break;

	  	  	  case 2:
	  	  		  if(HAL_GPIO_ReadPin(GPIOC,GPIO_PIN_15)==1){
  	  			  test2();
  	  			  servo(90,100,90,90);
  	  			  //ics_set_pos1(0,map_a(90,0,180,4833,10166));
  	  			  //ics_set_pos1(1,map_a(90,0,180,4833,10166));
  	 	  	  	  motor_move(0,0,0);
  	  			  oled_returnhome();
  	  			  oled_puts("testmode2 select");
  	  			  oled_setcursor(1,0);
  	  			  oled_puts("running         ");
  	  		  }
  	  		  else{
  	  			  servo(90,100,90,90);
  	  			  ics_set_pos1(0,map_a(90,0,180,4833,10166));
  	  			  ics_set_pos1(1,map_a(90,0,180,4833,10166));
  	 	  	  	  motor_move(0,0,0);
  	  			  oled_returnhome();
  	  			  oled_puts("testmode2 select");
  	  			  oled_setcursor(1,0);
  	  			  oled_puts("OK?     waiting ");
  	  			  HAL_Delay(5);
  	  		  HAL_GPIO_TogglePin(GPIOD,GPIO_PIN_10);
  	  		  }

	  	  	  break;

	  	  	  case 3:
	  	  		  if(HAL_GPIO_ReadPin(GPIOC,GPIO_PIN_15)==1){
  	  			  state_print();
  	  			  servo(90,100,90,90);
  	  			  ics_set_pos1(0,map_a(90,0,180,4833,10166));
  	  			  ics_set_pos1(1,map_a(90,0,180,4833,10166));
  	 	  	  	  motor_move(0,0,0);
  	  			  oled_returnhome();
  	  			  oled_puts("State     select");
  	  			  oled_setcursor(1,0);
  	  			  oled_puts("running         ");
  	  		  }
  	  		  else{
  	  			  servo(90,100,90,90);
  	  			  ics_set_pos1(0,map_a(90,0,180,4833,10166));
  	  			  ics_set_pos1(1,map_a(90,0,180,4833,10166));
  	 	  	  	  motor_move(0,0,0);
  	  			  oled_returnhome();
  	  			  oled_puts("State     select");
  	  			  oled_setcursor(1,0);
  	  			  oled_puts("OK?     waiting ");
  	  			  HAL_Delay(5);
  	  		  HAL_GPIO_TogglePin(GPIOD,GPIO_PIN_10);
  	  		  }

	  	  	  break;

	  	  	  default:
	  	  	  HAL_GPIO_TogglePin(GPIOD,GPIO_PIN_10);
	  	  		  servo(90,100,90,90);
	  	  	  	  motor_move(0,0,0);
	  	  	  	  oled_returnhome();
	  	  	  	  oled_puts("select mode     ");
	  	  	  	  oled_setcursor(1,0);
	  	  	  	  oled_puts("           SURF ");
	  	  	  	  HAL_Delay(5);
	  	  	  break;
	  	  	  }
  }
  /* USER CODE END 3 */

}

/**
  * @brief System Clock Configuration
  * @retval None
  */
void SystemClock_Config(void)
{

  RCC_OscInitTypeDef RCC_OscInitStruct;
  RCC_ClkInitTypeDef RCC_ClkInitStruct;
  RCC_PeriphCLKInitTypeDef PeriphClkInit;

    /**Initializes the CPU, AHB and APB busses clocks 
    */
  RCC_OscInitStruct.OscillatorType = RCC_OSCILLATORTYPE_HSI|RCC_OSCILLATORTYPE_HSE;
  RCC_OscInitStruct.HSEState = RCC_HSE_ON;
  RCC_OscInitStruct.HSEPredivValue = RCC_HSE_PREDIV_DIV1;
  RCC_OscInitStruct.HSIState = RCC_HSI_ON;
  RCC_OscInitStruct.HSICalibrationValue = 16;
  RCC_OscInitStruct.PLL.PLLState = RCC_PLL_ON;
  RCC_OscInitStruct.PLL.PLLSource = RCC_PLLSOURCE_HSE;
  RCC_OscInitStruct.PLL.PLLMUL = RCC_PLL_MUL9;
  if (HAL_RCC_OscConfig(&RCC_OscInitStruct) != HAL_OK)
  {
    _Error_Handler(__FILE__, __LINE__);
  }

    /**Initializes the CPU, AHB and APB busses clocks 
    */
  RCC_ClkInitStruct.ClockType = RCC_CLOCKTYPE_HCLK|RCC_CLOCKTYPE_SYSCLK
                              |RCC_CLOCKTYPE_PCLK1|RCC_CLOCKTYPE_PCLK2;
  RCC_ClkInitStruct.SYSCLKSource = RCC_SYSCLKSOURCE_PLLCLK;
  RCC_ClkInitStruct.AHBCLKDivider = RCC_SYSCLK_DIV1;
  RCC_ClkInitStruct.APB1CLKDivider = RCC_HCLK_DIV2;
  RCC_ClkInitStruct.APB2CLKDivider = RCC_HCLK_DIV1;

  if (HAL_RCC_ClockConfig(&RCC_ClkInitStruct, FLASH_LATENCY_2) != HAL_OK)
  {
    _Error_Handler(__FILE__, __LINE__);
  }

  PeriphClkInit.PeriphClockSelection = RCC_PERIPHCLK_USART1|RCC_PERIPHCLK_USART2
                              |RCC_PERIPHCLK_USART3|RCC_PERIPHCLK_UART4
                              |RCC_PERIPHCLK_I2C1|RCC_PERIPHCLK_TIM1
                              |RCC_PERIPHCLK_TIM8;
  PeriphClkInit.Usart1ClockSelection = RCC_USART1CLKSOURCE_PCLK2;
  PeriphClkInit.Usart2ClockSelection = RCC_USART2CLKSOURCE_PCLK1;
  PeriphClkInit.Usart3ClockSelection = RCC_USART3CLKSOURCE_PCLK1;
  PeriphClkInit.Uart4ClockSelection = RCC_UART4CLKSOURCE_PCLK1;
  PeriphClkInit.I2c1ClockSelection = RCC_I2C1CLKSOURCE_HSI;
  PeriphClkInit.Tim1ClockSelection = RCC_TIM1CLK_HCLK;
  PeriphClkInit.Tim8ClockSelection = RCC_TIM8CLK_HCLK;
  if (HAL_RCCEx_PeriphCLKConfig(&PeriphClkInit) != HAL_OK)
  {
    _Error_Handler(__FILE__, __LINE__);
  }

    /**Configure the Systick interrupt time 
    */
  HAL_SYSTICK_Config(HAL_RCC_GetHCLKFreq()/1000);

    /**Configure the Systick 
    */
  HAL_SYSTICK_CLKSourceConfig(SYSTICK_CLKSOURCE_HCLK);

  /* SysTick_IRQn interrupt configuration */
  HAL_NVIC_SetPriority(SysTick_IRQn, 0, 0);
}

/* USER CODE BEGIN 4 */


void main_run(){
		float calc=0;
		uint16_t digree_now;
		digree_now=line_get_data_poorly_made();
		if(line_get_marker()==10){
			marker_count_data++;
		}
		if(marker_count_data>=10){
			shyuukai=1;
			marker_count_data=0;
		}
		if(marker_count_data_ != marker_count_data){
			marker_count_data_=marker_count_data;
 			 TIM3->CNT=30000;
		}

	  if(digree_now<360){
		  if(marker_count_data==1 ){//垂直標的
			  if((TIM3->CNT)>29700 && (TIM3->CNT)<29830){
	  			   ics_set_pos1(0,map_a(120,0,180,4833,10166));
	  			  DFPlayer_playmp3(3);
			  }
			  else{
				  ics_set_pos1(0,map_a(90,0,180,4833,10166));
			  }
		  }
		  if(marker_count_data==2){//垂直標的
			  if((TIM3->CNT)>29700 && (TIM3->CNT)<29830){
	  			   ics_set_pos1(0,map_a(120,0,180,4833,10166));
			  }
			  else{
				  ics_set_pos1(0,map_a(90,0,180,4833,10166));
			  }
		  }
		  if(marker_count_data==3){//横移動  servo(90,100,90,90);
			  while((TIM3->CNT)>29900){//少し前出る
				  digree_now=line_get_data_poorly_made();
				  if(digree_now<180){
					 calc=digree_now*(sqrt(digree_now*2))/16;
					 HOUI_Correction(0);
					 motor_move(125,(uint16_t)calc,800);
				 }
				 else{
					 calc=(360-digree_now)*(sqrt((360-digree_now)*2))/16;
					 HOUI_Correction(0);
					 motor_move(125,(uint16_t)(360.0-calc),800);
				 }
			  }
			  	  	 motor_move(0,0,0);
					 TIM4->CNT=30000;
					 while(TIM4->CNT>29530){//真ん中まで移動
						 HOUI_Correction(0);
						 motor_move(125,270,500);
						 }
					 //射撃
					 if(shyuukai==0){
					 DFPlayer_playmp3(2);
					 motor1(1000,1000);
					 motor2(1000,1000);
					 motor3(1000,1000);
					 HAL_Delay(1000);
					 }

			  		  servo(90,30,30,90);
					 while(TIM4->CNT>29050){//端まで移動
						 HOUI_Correction(0);
						 motor_move(125,270,500);
						 }
					 while(line_get_data_poorly_made()>300){//対岸まで移動
						 HOUI_Correction(0);
						 motor_move(125,270,500);
					 }
			  		  servo(90,100,90,90);
				 motor1(1000,1000);
				 motor2(1000,1000);
				 motor3(1000,1000);
				 HOUI_Correction(180);//回転
				 while(marker_count_data==3){//マーカーまで移動
					 digree_now=line_get_data_poorly_made();
					 if(line_get_marker()==10){
						 marker_count_data++;
					 }
					 if(marker_count_data_ != marker_count_data){
						 marker_count_data_=marker_count_data;
						 TIM3->CNT=30000;
					 }
					 if(digree_now<180){
						 if(digree_now==90){calc=90;}
						 calc=digree_now*(sqrt(digree_now*2))/16;
						 HOUI_Correction(180);
						 motor_move(125,(uint16_t)calc,700);
					 }
					 else{
						 if(digree_now==270){calc=270;}
						 calc=(360-digree_now)*(sqrt((360-digree_now)*2))/16;
						 HOUI_Correction(180);
						 motor_move(125,(uint16_t)(360.0-calc),700);
					 }

				 }
				 TIM3->CNT=30000;
				 while((TIM3->CNT)>29800){//少し前出る
					 digree_now=line_get_data_poorly_made();
					 if(digree_now<180){
						 calc=digree_now*(sqrt(digree_now*2))/16;
						 HOUI_Correction(180);
						 motor_move(125,(uint16_t)calc,800);
					 }
					 else{
						 calc=(360-digree_now)*(sqrt((360-digree_now)*2))/16;
						 HOUI_Correction(180);
						 motor_move(125,(uint16_t)(360.0-calc),800);
					 }
				 }

		  }

		  if(marker_count_data==5){//右水平標的
			  if((TIM3->CNT)>29600 && (TIM3->CNT)<30000){
	  			   ics_set_pos1(0,map_a(120,0,180,4833,10166));
		  			  DFPlayer_playmp3(5);
			  }
			  else{
				  ics_set_pos1(0,map_a(90,0,180,4833,10166));
			  }
		  }
		  if(marker_count_data==6){//左水平標的
			  if((TIM3->CNT)>29600 && (TIM3->CNT)<30000){
	  			   ics_set_pos1(0,map_a(60,0,180,4833,10166));
			  }
			  else{
				  ics_set_pos1(0,map_a(90,0,180,4833,10166));
			  }
		  }
		  if(marker_count_data==7){//右水平標的
			  if((TIM3->CNT)>29600 && (TIM3->CNT)<30000){
	  			   ics_set_pos1(0,map_a(120,0,180,4833,10166));
		  			  DFPlayer_playmp3(4);
			  }
			  else{
				  ics_set_pos1(0,map_a(90,0,180,4833,10166));
			  }
		  }

		  if(marker_count_data==8){//左水平標的
			  if((TIM3->CNT)>29600 && (TIM3->CNT)<30000){
	  			   ics_set_pos1(0,map_a(60,0,180,4833,10166));
			  }
			  else{
				  ics_set_pos1(0,map_a(90,0,180,4833,10166));
			  }
		  }

		  if(marker_count_data==9){//横移動

			  while((TIM3->CNT)>29900){//少し前に出る
				  digree_now=line_get_data_poorly_made();
				  if(digree_now<180){
					 calc=digree_now*(sqrt(digree_now*2))/16;
					 HOUI_Correction(180);
					 motor_move(125,(uint16_t)calc,800);
				 }
				 else{
					 calc=(360-digree_now)*(sqrt((360-digree_now)*2))/16;
					 HOUI_Correction(180);
					 motor_move(125,(uint16_t)(360.0-calc),800);
				 }
			  }
		  	  	 motor_move(0,0,0);
				 TIM4->CNT=30000;
				 while(TIM4->CNT>29530){//真ん中まで移動
					 HOUI_Correction(180);
					 motor_move(125,270,500);
					 }
				 //射撃
				 if(shyuukai==0){
				 DFPlayer_playmp3(2);
				 motor1(1000,1000);
				 motor2(1000,1000);
				 motor3(1000,1000);
				 HAL_Delay(1000);
				 }

		  		  servo(90,30,30,90);
				 while(TIM4->CNT>29050){//端まで移動
						 HOUI_Correction(180);
						 motor_move(125,270,500);
						 }
					 while( line_get_data_poorly_made()>300){//対岸まで移動
						 HOUI_Correction(180);
						 motor_move(125,270,500);
					 }
					 motor1(1000,1000);
					 motor2(1000,1000);
					 motor3(1000,1000);
					 HAL_Delay(100);
				 HOUI_Correction(0);
		  		  servo(90,100,90,90);
				 while(marker_count_data==9){//マーカーまで移動
					 digree_now=line_get_data_poorly_made();
					 		if(line_get_marker()==10){
					 			marker_count_data++;
					 		}
					 		if(marker_count_data_ != marker_count_data){
					 			marker_count_data_=marker_count_data;
					  			 TIM3->CNT=30000;
					 		}
					 		if(digree_now<180){
					 			calc=digree_now*(sqrt(digree_now*2))/16;
					 			if(digree_now==90){calc=90;}
					 			HOUI_Correction(0);
					 			motor_move(125,(uint16_t)calc,700);
					 		}
					 		else{
					 			calc=(360-digree_now)*(sqrt((360-digree_now)*2))/16;
					 			if(digree_now==270){calc=270;}
					 			HOUI_Correction(0);
					 			motor_move(125,(uint16_t)(360.0-calc),700);
					 		}

				 }				 TIM3->CNT=30000;
				 while((TIM3->CNT)>29800){//少し前出る
					 digree_now=line_get_data_poorly_made();
					 if(digree_now<180){
						 calc=digree_now*(sqrt(digree_now*2))/16;
						 HOUI_Correction(0);
						 motor_move(125,(uint16_t)calc,800);
					 }
					 else{
						 calc=(360-digree_now)*(sqrt((360-digree_now)*2))/16;
						 HOUI_Correction(0);
						 motor_move(125,(uint16_t)(360.0-calc),800);
					 }
				 }
		  }


			  if(marker_count_data<3 ||marker_count_data==10){//0°方向直進移動
			  	  if(digree_now<180){
				  calc=digree_now*(sqrt(digree_now*2))/16;
				  if(digree_now==90){calc=90;}
				  HOUI_Correction(0);
				  motor_move(125,(uint16_t)calc,800);
			  	  }
			  	  else{
					  calc=(360-digree_now)*(sqrt((360-digree_now)*2))/16;
					  if(digree_now==270){calc=270;}
					  HOUI_Correction(0);
					  motor_move(125,(uint16_t)(360.0-calc),800);
			  	  }
			  }
			  else{
				  if(digree_now<180){//180°方向直進移動
				  calc=digree_now*(sqrt(digree_now*2))/16;
				  if(digree_now==90){calc=90;}
				  HOUI_Correction(180);
				  motor_move(125,(uint16_t)calc,800);
			  	  }
			  	  else{
					  calc=(360-digree_now)*(sqrt((360-digree_now)*2))/16;
					  if(digree_now==270){calc=270;}
					  HOUI_Correction(180);
					  motor_move(125,(uint16_t)(360.0-calc),800);
			  	  }
			  }

				  xprintf(" m1=%d", TIM3->CNT);
				  xprintf(" m2=%d", TIM4->CNT);
				  xprintf(" m3=%d", TIM8->CNT);
				  xprintf(" count=%d",marker_count_data);
				  xprintf(" digree=%d\r\n",digree_now);

	  }
	  else{motor_move(0,0,0);}
	  HAL_GPIO_TogglePin(GPIOD,GPIO_PIN_10);

}
void test2(){/*
	  xprintf(" m1=%d", TIM3->CNT);
	  xprintf(" m2=%d", TIM4->CNT);
	  xprintf(" m3=%d", TIM8->CNT);
	ics_set_pos1(0,map_a(120,0,180,4833,10166));*/

	float calc=0;
	uint16_t digree_now;
	digree_now=line_get_data_poorly_made();
		  servo(90,100,90,90);
	  while((TIM3->CNT)>29900){//少し前出る
		  digree_now=line_get_data_poorly_made();
		  if(digree_now<180){
			 calc=digree_now*(sqrt(digree_now*2))/16;
			 HOUI_Correction(0);
			 motor_move(125,(uint16_t)calc,800);
		 }
		 else{
			 calc=(360-digree_now)*(sqrt((360-digree_now)*2))/16;
			 HOUI_Correction(0);
			 motor_move(125,(uint16_t)(360.0-calc),800);
		 }
	  }
	  	  	 motor_move(0,0,0);
			 TIM4->CNT=30000;
			 while(TIM4->CNT>29530){//真ん中まで移動
				 HOUI_Correction(0);
				 motor_move(125,270,500);
				 }
			 //射撃

			 motor1(1000,1000);
			 motor2(1000,1000);
			 motor3(1000,1000);
			 HAL_Delay(100);
			  Txbuf[0]=0xFA;
			  xprintf(" Rx[0]=%d", Rxbuf[0]);
			  xprintf(" Rx[1]=%d", Rxbuf[1]);
			  xprintf(" Rx[2]=%d", Rxbuf[2]);
			  xprintf(" data[0]=%d", data[0]);
			  xprintf(" data[1]=%d", data[1]);
			  	  xprintf("\r\n");
			  HAL_UART_Transmit(&huart2,(uint8_t *)Txbuf,1,0xFF);
			  HAL_UART_Transmit(&huart2,(uint8_t *)Txbuf,1,0xFF);
			  HAL_UART_Transmit(&huart2,(uint8_t *)Txbuf,1,0xFF);
			  HAL_UART_Transmit(&huart2,(uint8_t *)Txbuf,1,0xFF);
			  HAL_UART_Transmit(&huart2,(uint8_t *)Txbuf,1,0xFF);
			  HAL_UART_Transmit(&huart2,(uint8_t *)Txbuf,1,0xFF);
			  HAL_UART_Transmit(&huart2,(uint8_t *)Txbuf,1,0xFF);
			  HAL_UART_Transmit(&huart2,(uint8_t *)Txbuf,1,0xFF);
			  HAL_UART_Transmit(&huart2,(uint8_t *)Txbuf,1,0xFF);
			  HAL_UART_Transmit(&huart2,(uint8_t *)Txbuf,1,0xFF);
			  int cunttime_syageki=0;
			 while(((data[1]&0b00100000)>>5)==0 && cunttime_syageki<600){
				 ics_set_pos1(1,map_a(data[0],0,180,4833,10166));
				  xprintf(" Rx[0]=%d", Rxbuf[0]);
				  xprintf(" Rx[1]=%d", Rxbuf[1]);
				  xprintf(" Rx[2]=%d", Rxbuf[2]);
				  xprintf(" data[0]=%d", data[0]);
				  xprintf(" data[1]=%d", data[1]);
				  xprintf(" time=%d", cunttime_syageki);
				  	  xprintf("\r\n");
				  	cunttime_syageki++;
				  	if((data[1]&0b00100000)>>5)==1){
				  		cunttime_syageki=1000;
				  	}
			 }
			 DFPlayer_playmp3(2);
			 HAL_Delay(1000);

			 ics_set_pos1(1,map_a(90,0,180,4833,10166));

	  		  servo(90,30,30,90);
			 while(TIM4->CNT>29200){//端まで移動
				 HOUI_Correction(0);
				 motor_move(125,270,500);
				 }
			 while(line_get_data_poorly_made()>300){//対岸まで移動
				 HOUI_Correction(0);
				 motor_move(125,270,500);
			 }
	  		  servo(90,100,90,90);
		 motor1(1000,1000);
		 motor2(1000,1000);
		 motor3(1000,1000);
		 HOUI_Correction(180);//回転
		 while(marker_count_data==1){//マーカーまで移動
			 digree_now=line_get_data_poorly_made();
			 if(line_get_marker()==10){
				 marker_count_data++;
			 }
			 if(marker_count_data_ != marker_count_data){
				 marker_count_data_=marker_count_data;
				 TIM3->CNT=30000;
			 }
			 if(digree_now<180){
				 if(digree_now==90){calc=90;}
				 calc=digree_now*(sqrt(digree_now*2))/16;
				 HOUI_Correction(180);
				 motor_move(125,(uint16_t)calc,800);
			 }
			 else{
				 if(digree_now==270){calc=270;}
				 calc=(360-digree_now)*(sqrt((360-digree_now)*2))/16;
				 HOUI_Correction(180);
				 motor_move(125,(uint16_t)(360.0-calc),800);
			 }

		 }
		 	 HAL_Delay(100000000);
	  HAL_GPIO_TogglePin(GPIOD,GPIO_PIN_10);
}


